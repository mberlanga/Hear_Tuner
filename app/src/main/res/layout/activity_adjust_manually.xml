    <TextView
        android:id="@+id/band0_min_range"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/min_range"
        app:layout_constraintStart_toStartOf="@+id/fBandAdjuster0"
        app:layout_constraintTop_toBottomOf="@+id/fBandAdjuster0" />

    <TextView
        android:id="@+id/band0_max_range"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
