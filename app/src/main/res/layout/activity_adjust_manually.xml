    <TextView
        android:id="@+id/title_manual_entry_screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:text="@string/title_manual_profile_entry"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/band0_center_frequency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="@string/center_frequency"
        app:layout_constraintBottom_toTopOf="@+id/fBandAdjuster0"
        app:layout_constraintEnd_toEndOf="@+id/fBandAdjuster0"
        app:layout_constraintStart_toStartOf="@+id/fBandAdjuster0" />

