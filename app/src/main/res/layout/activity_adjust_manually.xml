    <TextView
        android:id="@+id/band3_max_range"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/max_range"
        app:layout_constraintEnd_toEndOf="@+id/fBandAdjuster3"
        app:layout_constraintTop_toBottomOf="@+id/fBandAdjuster3" />

    <TextView
        android:id="@+id/band4_max_range"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/max_range"
        app:layout_constraintEnd_toEndOf="@+id/fBandAdjuster4"
        app:layout_constraintTop_toBottomOf="@+id/fBandAdjuster4" />

    <TextView
        android:id="@+id/band2_max_range"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/max_range"
        app:layout_constraintEnd_toEndOf="@+id/fBandAdjuster2"
