        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="@string/center_frequency"
        app:layout_constraintBottom_toTopOf="@+id/fBandAdjuster3"
        app:layout_constraintEnd_toEndOf="@+id/fBandAdjuster3"
        app:layout_constraintStart_toStartOf="@+id/fBandAdjuster3" />

</android.support.constraint.ConstraintLayout>